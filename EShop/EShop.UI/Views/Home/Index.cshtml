@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    
    <fieldset>
        <legend></legend>
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <div id="imageCarousel" class="carousel slide" data-interval="2000"
                         data-ride="carousel" data-pause="hover" data-wrap="true">

                        <ol class="carousel-indicators">
                            <li data-target="#imageCarousel" data-slide-to="0" class="active"></li>
                            <li data-target="#imageCarousel" data-slide-to="1"></li>
                            <li data-target="#imageCarousel" data-slide-to="2"></li>
                           
                        </ol>
                       
                        <div class="carousel-inner">
                            <div class="item active">
                                <img src="~/css/images/photo- shoe store.jpg"  class="img-responsive"/>
                                <div class="carousel-caption">
                                    <h3></h3>
                                </div>
                            </div>
                            <div class="item">
                                <img src="~/css/images/photo-shoestor2.jpg" class="img-responsive" />
                                <div class="carousel-caption">
                                    <h3></h3>
                                   
                                </div>
                            </div>
                            <div class="item">
                                <img src="~/css/images/photo-shoestor3.jpg" class="img-responsive">
                                <div class="carousel-caption">
                                    <h3></h3>
                                   
                                </div>
                            </div>
                           
                        </div>

                        <a href="#imageCarousel" class="carousel-control left" data-slide="prev">
                            <span class="glyphicon glyphicon-hand-left"></span>
                        </a>
                        <a href="#imageCarousel" class="carousel-control right" data-slide="next">
                            <span class="glyphicon glyphicon-hand-right"></span>
                        </a>
                    </div>

                </div>
            </div>
        </div>
    </fieldset>
   
  
    <img src="~/css/images/photo3.avif" width="360" height="200" style="vertical-align:bottom" />
    
</div>
